<Window x:Class="robotX.Progresser"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:robotX"
        mc:Ignorable="d"
        Title="拷贝工程" Height="150" Width="500" WindowStartupLocation="CenterOwner">
    <Grid>
        <StackPanel>
            <ProgressBar Name="Bar"  Height="30" Maximum="100" Minimum="0"  Margin="10"  Opacity="0.5" Value="30" ValueChanged="ProgressBar_ValueChanged"/>
            <TextBlock Name="FileObj" Height="40" FontSize="20" Margin="0 20 0 0"></TextBlock>
        </StackPanel>

    </Grid>
</Window>
